{
    "name": "ao-contracts",
    "version": "1.0.0",
    "description": "",
    "main": "build/index.js",
    "scripts": {
        "watch": "tsc --watch",
        "ganache": "./scripts/runGanache.sh",
        "compileAndDeploy": "node build/compileAndDeploy.js -s ~/solc-0.5.11 -o ./abi -i ./sol/",
        "compileAndDeployProd": "node build/compileAndDeploy.js -s ~/solc-0.5.11 -o ./abi -i ./sol/ -m",
        "build": "tsc",
        "test-oou": "NODE_ENV=local-dev jest ts/__tests__/OneOfUs.test.ts",
        "test-oou-debug": "NODE_ENV=local-dev node --inspect-brk ./node_modules/.bin/jest --testPathPattern=__tests__/OneOfUs.test.ts",
        "test-surrogeth": "NODE_ENV=local-dev jest ts/__tests__/Surrogeth.test.ts",
        "test-surrogeth-debug": "NODE_ENV=local-dev node --inspect-brk ./node_modules/.bin/jest --testPathPattern=__tests__/Surrogeth.test.ts"
    },
    "author": "Koh Wei Jie",
    "_moduleAliases": {
      "@ao-contracts": "."
    },
    "dependencies": {
        "ao-config": "1.0.0",
        "circom": "0.0.33",
        "circomlib": "0.0.18",
        "ethers": "4.0.37",
        "module-alias": "^2.2.0",
        "ganache-cli": "^6.7.0",
        "jest": "^24.9.0",
        "libsemaphore": "^0.0.6",
        "openzeppelin-eth": "^2.1.3",
        "snarkjs": "0.1.17",
        "ts-jest": "^24.1.0"
    },
    "devDependencies": {
        "@types/jest": "^24.0.15",
        "@types/node": "^12.7.7",
        "argparse": "1.0.10",
        "shelljs": "0.8.3",
        "axios": "0.19.0"
    }
}
